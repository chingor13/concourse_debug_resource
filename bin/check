#!/usr/bin/ruby

require 'json'

def log(message)
  $stderr.puts(message.inspect)
end

input = $stdin.read
data = JSON.parse(input)
log("checking...")
log(data)

output = {
  "version" => { "foo" => "bar" },
  "metadata" => [
    { "name" => "asdf", "value" => "qwer" },
    { "name" => "zxcv", "value" => "uiop" }
  ]
}
puts output.to_json
