#!/usr/bin/ruby

require 'json'

def log(message)
  $stderr.puts(message.inspect)
end

input = $stdin.read
data = JSON.parse(input)
log("out...")
log(data)

puts %{{
  "version": { "ref": "61cebf" },
  "metadata": [
    { "name": "commit", "value": "61cebf" },
    { "name": "author", "value": "Mick Foley" }
  ]
}}
